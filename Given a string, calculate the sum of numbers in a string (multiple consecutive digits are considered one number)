

class Main {
    public static void main(String[] args) {
     String str="123xyz";
     String str2="";
     int sum=0;
     for(int i=0;i<str.length();i++)
     {
         char c = str.charAt(i);
         if(Character.isDigit(c))
         {
             str2+=c;
         }
         else
         {
             sum+=Integer.parseInt(str2);
             str2="0";
         }
     }
     sum+=Integer.parseInt(str2);
     System.out.println(sum);
    }
}
