class Main {
    public static void main(String[] args) {
        String str1 = "goforward";
        String str2 = "forward";
        int count = -1;
        for (int i = 0; i <= str1.length() - str2.length(); i++) { 
            if (str1.substring(i, i + str2.length()).equals(str2)) {
               count=i;
            }
        }
        if(count!=-1)
        {
            System.out.println(count);
        }
        else
        {
          System.out.println(-1);
        }

    }
}
