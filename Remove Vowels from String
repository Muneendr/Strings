
import java.util.*;
class Main {
    public static void main(String[] args) {
      String str="Hello World";
      String str1="";
      for(int i=0;i<str.length();i++)
      {
          char c=str.charAt(i);
          char d=Character.toLowerCase(c);
          if(d!='a' && d!='e' && d!='i' && d!='o' && d!='u')
          {
              System.out.println(d);
              str1+=c;
          }
      }
        System.out.println(str1);
    }
}
