class Main {
    public static void main(String[] args) {
        String s = "MuneendrA iS A gooD BoY";
        int n=s.length();
        String cap="";
        for(int i=0;i<n;i++)
        {
            int a = (int) s.charAt(i);
            if(a>64 && a<91)
            {
               cap+= s.substring(i,i+1).toLowerCase();
            }
            else if(a>96 && a<123)
            {
                cap+= s.substring(i,i+1).toUpperCase();
            }
            else
            {
                cap+=" ";
            }
        }
        System.out.println(cap);
    }
}
